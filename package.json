{
  "name": "youbencha",
  "version": "0.1.6-beta",
  "description": "A friendly, developer-first CLI framework for evaluating agentic coding tools",
  "main": "dist/cli/index.js",
  "bin": {
    "youbencha": "dist/cli/index.js",
    "yb": "dist/cli/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc && node -e \"const fs = require('fs'); fs.mkdirSync('dist/evaluators/prompts', {recursive: true}); fs.copyFileSync('src/evaluators/prompts/agentic-judge.template.md', 'dist/evaluators/prompts/agentic-judge.template.md');\"",
    "test": "jest",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "prepack": "npm run build"
  },
  "keywords": [
    "agent",
    "evaluation",
    "testing",
    "cli",
    "benchmark",
    "ai",
    "copilot",
    "automation",
    "code-generation",
    "llm"
  ],
  "author": "youBencha",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/youbencha/youbencha-cli.git"
  },
  "bugs": {
    "url": "https://github.com/youbencha/youbencha-cli/issues"
  },
  "homepage": "https://github.com/youbencha/youbencha-cli#readme",
  "files": [
    "dist/",
    "examples/",
    "README.md",
    "GETTING-STARTED.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "diff": "^5.2.0",
    "fs-extra": "^11.3.2",
    "ora": "^8.2.0",
    "simple-git": "^3.30.0",
    "yaml": "^2.8.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/diff": "^5.2.3",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.24",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  }
}
